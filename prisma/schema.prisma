// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  // url と directUrl は削除して provider だけ残す
}

model Food {
  id        Int      @id @default(autoincrement())
  name      String
  quantity  String
  expiry    String?
  createdAt DateTime @default(now())
}